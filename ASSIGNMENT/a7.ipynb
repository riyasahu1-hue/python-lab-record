   "source": [
    "\n",
    "\n",
    "# Taking input from user\n",
    "items = list(map(int, input(\"Enter list items separated by space: \").split()))\n",
    "\n",
    "# Removing duplicates using set and sorting\n",
    "unique_sorted_list = sorted(set(items))\n",
    "\n",
    "# Displaying result\n",
    "print(\"Sorted list without duplicates:\", unique_sorted_list)"
   ]
  }
